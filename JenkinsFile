pipeline {
    agent any

    stages {
        
        stage('Build')
        {
            steps
            {
                echo 'Building app David RA'
            }
        }
        
        stage('Developing')
        {
            steps
            {
                echo 'Developing app David RA'
            }
        }
        
        stage('Sleeping')
        {
            steps
            {
                sleep 30
                echo 'Sleeping'
            }
        }
        
        stage('Test')
        {
            steps
            {
                echo 'Testing app David RA'
            }
        }
    }
}
